{
  "name": "@gitgraph/js",
  "version": "1.4.0",
  "description": "Draw pretty git graphs in the browser",
  "author": "Nicolas Carlo <nicolascarlo.espeon@gmail.com>",
  "contributors": [
    {
      "name": "Fabien Bernard",
      "email": "fabien0102@gmail.com"
    }
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nicoespeon/gitgraph.js.git"
  },
  "bugs": {
    "url": "https://github.com/nicoespeon/gitgraph.js/issues"
  },
  "keywords": [
    "git",
    "graph",
    "javascript"
  ],
  "homepage": "https://github.com/nicoespeon/gitgraph.js#readme",
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "main": "./lib/index.js",
  "typings": "./lib/index.d.ts",
  "directories": {
    "lib": "lib"
  },
  "files": [
    "lib"
  ],
  "scripts": {
    "prewatch": "rimraf ./lib",
    "watch": "tsc -w",
    "build": "run-s build:*",
    "build:clear": "rimraf ./lib",
    "build:tsc": "tsc",
    "build:bundle": "rollup -c rollup.config.js",
    "prepare": "npm run build",
    "version": "auto-changelog -p -l 0 --tag-prefix @gitgraph/js@ && git add CHANGELOG.md"
  },
  "dependencies": {
    "@gitgraph/core": "1.5.0"
  },
  "devDependencies": {
    "@types/node": "20.14.2",
    "auto-changelog": "2.4.0",
    "npm-run-all": "4.1.5",
    "rimraf": "5.0.7",
    "rollup": "4.18.0",
    "rollup-plugin-commonjs": "9.3.4",
    "rollup-plugin-node-resolve": "4.2.3",
    "uglify-es": "3.3.9"
  }
}
